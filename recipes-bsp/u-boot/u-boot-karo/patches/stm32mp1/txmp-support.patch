+static void print_mac_from_fuse(void)
+{
+	u32 fuse[2];
+	u8 mac[ETH_ALEN];
+
+	fuse_read(0, 57, &fuse[0]);
+	fuse_read(0, 58, &fuse[1]);
+	memcpy(mac, fuse, ETH_ALEN);
+
+	if (is_valid_ethaddr(mac))
+		printf("MAC addr from fuse: %pM\n", mac);
+}
