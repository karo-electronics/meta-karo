+	/* probe all PINCTRL for hog */
+	for (uclass_first_device(UCLASS_PINCTRL, &dev);
+	     dev;
+	     uclass_next_device(&dev)) {
+		debug("probe pincontrol = %s\n", dev->name);
+	}
+
+	sysconf_init();
+
+	if (ctrlc()) {
+		printf("<CTRL-C> detected; safeboot enabled\n");
+	}
+
+	show_bmp_logo();
+	board_key_check();
+	txmp_setup_led();
+
+	board_usbotg_init();
+
+	return 0;
+}
