+static struct tx8m_esdhc_cfg *to_tx8m_esdhc_cfg(struct fsl_esdhc_cfg *priv)
+{
+	for (size_t i = 0; i < ARRAY_SIZE(tx8mn_sdhc_cfgs); i++) {
+		struct tx8m_esdhc_cfg *cfg = &tx8mn_sdhc_cfgs[i];
